# OpenAI API Key (required for CrewAI)
OPENAI_API_KEY=your_openai_api_key_here

# BrowserBase Configuration (required for web scraping)
BROWSERBASE_API_KEY=your_browserbase_api_key_here
BROWSERBASE_PROJECT_ID=your_browserbase_project_id_here

# Weights & Biases (optional, for experiment tracking)
WANDB_API_KEY=your_wandb_api_key_here
WANDB_PROJECT=patienthero
WANDB_ENTITY=your_wandb_username

# Application Settings
LOG_LEVEL=INFO
MAX_CONCURRENT_REQUESTS=5
REQUEST_TIMEOUT=30

# Uncomment and set these if you need to use a proxy
# HTTP_PROXY=http://your-proxy:port
# HTTPS_PROXY=http://your-proxy:port

# Environment Variables Template
# Copy this file to .env.local and fill in your values

# Primary AI Service Configuration (Fly.io Med42-8B)
NEXT_PUBLIC_AI_API_URL=https://patienthero-med42.fly.dev
NEXT_PUBLIC_AI_API_KEY=your_api_key_here_if_needed

# Fallback AI Service Configuration (W&B DeepSeek R1)
NEXT_PUBLIC_WANDB_API_URL=http://localhost:8001
# For production W&B deployment, use your deployed URL:
# NEXT_PUBLIC_WANDB_API_URL=https://your-wandb-deployment.com

# Alternative AI Provider Keys (optional fallbacks)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Feature Flags (optional)
NEXT_PUBLIC_ENABLE_VOICE=false
NEXT_PUBLIC_ENABLE_FILE_UPLOAD=false
